{
  "manifest_version": 2,
  "name": "Cyberscholars Scheduling",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.4.1.min.js", "content.js"]
    }
  ],
  "web_accessible_resources" : ["scrape_and_analyze.js"],
  "browser_action": {
    "default_icon": "icon.png"
  }
}